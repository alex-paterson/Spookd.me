<div class="row">
	<% stories.each do |story| %>
		<%= render(partial: 'stories/story', locals: {story: story}) %>
	<% end %>
	<div class="home-articles">
		<div class="article-nibs-container">
			<hr>
			<% articles.each do |article| %>
				<%= render(partial: 'articles/article_nib', locals: {article: article}) %>
				<hr>
			<% end %>
		</div>
	</div>
</div>
